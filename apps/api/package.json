{
  "name": "api",
  "engines": {
    "node": "16.16"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "esbuild src/server.ts --bundle --platform=node --outfile=dist/server.js",
    "start": "node dist/server.js",
    "lint": "eslint src",
    "lint:fix": "pnpm lint --fix",
    "test": "jest --coverage --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "clean": "git clean -xdf .expo .turbo node_modules",
    "type-check": "tsc",
    "scalingo-postbuild": "npx prisma migrate deploy --schema=./dist/schema.prisa && npm run build",
    "scalingo:build": "npm run build",
    "scalingo:start": "npm start"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/node-cron": "^3.0.7",
    "@types/swagger-ui-express": "^4.1.3",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.18",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "node-cron": "^3.0.2",
    "socket.io": "^4.6.1",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/supertest": "^2.0.12",
    "babel": "^6.23.0",
    "database": "*",
    "dotenv-cli": "^7.2.1",
    "eslint-config-wolfo": "*",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "tsconfig": "*",
    "tsx": "^3.12.3",
    "types": "*",
    "typescript": "^4.9.5"
  }
}
