{
  "name": "database",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "dependencies": {
    "@prisma/client": "^4.12.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "prisma": "^4.12.0",
    "tsconfig": "workspace:*"
  },
  "scripts": {
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push --skip-generate",
    "db:migrate": "pnpm with-env prisma migrate dev",
    "with-env": "dotenv -e ../../.env --"
  }
}