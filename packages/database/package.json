{
  "name": "database",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "dependencies": {
    "@prisma/client": "^4.12.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "prisma": "^4.12.0",
    "tsconfig": "workspace:*"
  },
  "scripts": {
    "clean": "git clean -xdf .expo .turbo node_modules",
    "docker:init": "pnpm with-env docker-compose up -d",
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push --skip-generate",
    "db:migrate": "pnpm with-env prisma migrate dev",
    "db:reset": "pnpm with-env prisma migrate reset",
    "db:deploy": "pnpm with-env prisma migrate deploy",
    "db:studio": "pnpm with-env prisma studio",
    "lint": "pnpm with-env prisma validate && prisma format",
    "with-env": "dotenv -e ../../.env --",
    "test": "dotenv -e ../../.env.test -- prisma db push"
  }
}
