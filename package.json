{
  "name": "projet_acolweb",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "16.16"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "scalingo-postbuild": "turbo run scalingo:build",
    "scalingo:start": "turbo run scalingo:start",
    "start": "turbo run start",
    "type-check": "turbo run type-check",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test:frontend": "turbo run dev & turbo run test:frontend",
    "test:frontend:ci": "turbo run dev & turbo run test:frontend:ci",
    "test:backend": "turbo run test:backend",
    "db:push": "turbo run db:push",
    "db:generate": "turbo run db:generate",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "husky": "^8.0.3",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "prettier": "^2.8.7",
    "turbo": "^1.9.3"
  }
}
